<html>
  <head>
    <meta charset="utf-8" />
    <title>Lista de Productos</title>
  </head>
  <body>
    <h1>Lista de Productos</h1>
    <table>
      <thead>
        <tr>
          <!-- Eliminar la columna del Id -->
          <th>Nombre</th>
          <th>Descripción</th>
          <th>Precio</th>
          <th>Código</th>
          <th>Stock</th>
          <th>Categoría</th>
        </tr>
      </thead>
      <tbody>
        {{#each products}}
          <tr>
            <!-- Eliminar el <td> correspondiente al Id -->
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
            <td>{{this.code}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.category}}</td>
          </tr>
        {{/each}}
      </tbody>

    </table>
  </body>
</html>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>